{
  "name": "chibipos",
  "productName": "chibipos",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".vite/build/main.js",
  "type": "module",
  "scripts": {
    "start": "electron-forge start",
    "build:electron": "vite build --config vite.main.config.mjs",
    "build:svelte": "vite build --config vite.renderer.config.mjs",
    "build:preload": "vite build --config vite.preload.config.mjs",
    "build:all": "npm run build:svelte && npm run build:preload && npm run build:electron",
    "build": "npm run build:all",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "dev": "electron-forge start",
    "prepare": "svelte-kit sync",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "keywords": [],
  "author": "guillermo",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "electron": "35.0.2",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "vite": "^6.3.5",
    "sass": "^1.87.0"
  },
  "dependencies": {
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.8",
    "crypto-js": "^4.2.0",
    "electron-squirrel-startup": "^1.0.1",
    "escpos": "^3.0.0-alpha.6",
    "escpos-network": "^3.0.0-alpha.5",
    "escpos-usb": "^3.0.0-alpha.4",
    "fs-extra": "^11.3.0",
    "globals": "^16.0.0",
    "gsap": "file:gsap-bonus.tgz",
    "pg-hstore": "^2.3.4",
    "sequelize": "^7.0.0-next.1",
    "sqlite3": "^5.1.7",
    "svelte": "^5.0.0",
    "svelte-preprocess": "^6.0.3",
    "wait-on": "^8.0.3"
  }
